{
    "name": "fsimchen/example-php-composer",
    "description": "Simple PHP project using Composer.",
    "keywords": ["php", "composer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Felipe Simchen",
            "email": "felipe.simchen@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "ext-openssl": "*",
        "nesbot/carbon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}